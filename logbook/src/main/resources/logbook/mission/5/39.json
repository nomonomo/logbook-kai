/*遠洋潜水艦作戦*/
{
    "operator": "AND",
    "conditions": [
        {"type": "艦隊", "count_type": "レベル", "value": 180},
        {
            "operator": "AND",
            "conditions": [
                {"type": "艦娘", "level": 3, "order": 1}
            ]
        },
        {
            "operator": "AND",
            "conditions": [
                {"type": "艦娘", "ship_type" :["潜水母艦"]},
                {"type": "艦娘", "ship_type" :["潜水艦", "潜水空母"]},
                {"type": "艦娘", "ship_type" :["潜水艦", "潜水空母"]},
                {"type": "艦娘", "ship_type" :["潜水艦", "潜水空母"]},
                {"type": "艦娘", "ship_type" :["潜水艦", "潜水空母"]}
            ]
        }
    ]
}